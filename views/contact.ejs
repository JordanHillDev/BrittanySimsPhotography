<form action="https://formsubmit.co/7c8ac3b459a235de2bc33acfad8208fe" id="contactForm" method="POST">
    <!-- honeypot -->
    <input type="text" name="_honey" style="display: none;">
    <!-- disable captcha -->
    <input type="hidden" name="_captcha" value="false">
    <!-- redirects to page in value after submitted -->
    <input type="hidden" name="_next" value="long url of page you want to go to">
    
    <div>
        <label for="name">Name</label>
        <input type="text" id="name" required>
    </div>
    <div>
        <label for="email">Email</label>
        <input type="email" id="email" required>
    </div>
    <div>
        <label for="message">Message</label>
        <input type="text" id="message" required>
    </div>
    <input type="submit" value="Send Message">
</form>